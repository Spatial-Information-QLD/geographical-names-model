PREFIX schema: <https://schema.org/>
PREFIX sh: <http://www.w3.org/ns/shacl#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX time: <http://www.w3.org/2006/time#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>


<https://linked.data.gov.au/def/addr/validator>
    a owl:Ontology ;
    skos:historyNote "This model validator was made for the Queensland Department of Resources, Spatial Information unit to validate Place Names data with, according to the Geographical Names Model."@en ;
    owl:versionIRI gn:1.0.0 ;
    owl:versionInfo "1.0.0" ;
    skos:definition "This validator is a Shapes Constraint Language (SHACL) resource that can be used to check the conformance of RDF data to the Geographical Names Model (<https://linked.data.gov.au/def/gn>)."@en ;
    skos:prefLabel "Geographical Names Model Validator"@en ;
    schema:codeRepository "https://github.com/Spatial-Information-QLD/geographical-names-model/"^^xsd:anyURI ;
    schema:copyrightHolder <https://linked.data.gov.au/org/qsi> ;
    schema:copyrightNotice "(c) The State of Queensland (Department of Resources) 2021-2025" ;
    schema:creator <https://orcid.org/0000-0002-8742-7730> ;
    schema:dateCreated "2021-12-16"^^xsd:date ;
    # schema:dateIssued "0000-00-00"^^xsd:date ;
    schema:dateModified "2025-11-10"^^xsd:date ;
    schema:license "https://creativecommons.org/licenses/by/4.0/"^^xsd:anyURI ;
    schema:publisher <https://linked.data.gov.au/org/qsi> ;
.

<https://linked.data.gov.au/org/qsi>
    a schema:Organization ;
    schema:name "Spatial Information Queensland"@en ;
    schema:url "https://qldspatial.information.qld.gov.au"^^xsd:anyURI ;
.

<https://orcid.org/0000-0002-8742-7730>
    a schema:Person ;
    schema:email "nick@kurrawong.ai"^^xsd:anyURI ;
    schema:name "Nicholas J. Car"@en ;
    schema:memberOf <https://kurrawong.ai> ;
.

:geo-object-present
    a sh:NodeShape ;
    sh:targetClass gn:GeographicalName ;
    sh:property [
        sh:path cn:isNameFor ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1;
        sh:minCount 1;
    ] ;
    sh:message "Each Geographical Name must indicate an IRI with the property cn:isNameFor" ;
.

:geocode-present
    a sh:NodeShape ;
    sh:targetClass gn:GeographicalName ;
    sh:property [
        sh:path cn:hasGeocode ;
        sh:minCount 1;
        sh:nodeKind sh:BlankNodeOrIRI ;
    ] ;
    sh:message "Each Geographical Name must indicate at least one geocode with the property gn:hasGeocode which cannot be a literal" ;
.

:geocode-parts
    a sh:NodeShape ;
    sh:targetObjectsOf cn:hasGeocode ;
    sh:property [
        sh:path schema:value ;
        sh:class geo:Geometry ;
        sh:minCount 1;
        sh:maxCount 1;
    ] ,
    [
        sh:path schema:roleName ;
        sh:nodeKind sh:IRI ;
        sh:minCount 1;
        sh:maxCount 1;
    ] ;
    sh:message "Each cn:hasGeocode predicate must indicate an object with exactly one schema:value predicate indicating exactly one geo:Geometry object and one schema:roleName predicate indicating an IRI" ;
.

:gn-parts-1
    a sh:NodeShape ;
    sh:targetClass gn:GeographicalName ;
    sh:property [
        sh:path cn:hasNamePart ;
        sh:minCount 1;
    ] ;
    sh:message "Each Geographical Name must have at least one name part indicated with cn:hasNamePart which cannot be a literal" ;
.

:gn-parts-each
    a sh:NodeShape ;
    sh:targetObjectsOf schema:hasPart ;
    sh:property [
        sh:path schema:value ;
        sh:nodeKind IRIOrLiteral ;
        sh:minCount 1;
        sh:maxCount 1;
    ] ,
    [
        sh:path schema:roleName ;
        sh:nodeKind sh:IRI ;
        sh:minCount 1;
        sh:maxCount 1;
    ] ;
    sh:message "Each part of a Geographical Name must indicate exactly one schema:value predicate indicating exactly one literal value or a cn:CompoundName object and one schema:roleName predicate indicating an IRI" ;
.

:gn-role-vocab
    a sh:NodeShape ;
    sh:targetObjectsOf schema:roleName ;
    sh:property [
        sh:path skos:inScheme ;
        sh:value <https://linked.data.gov.au/def/gn-part-types> ;
        sh:minCount 1;
        sh:maxCount 1;
    ] ;
    sh:message "Each schema:roleName must indicate that it is within the Geographical Names Part Types vocabulary by indicating the predicate skos:inScheme indicating an object value of <https://linked.data.gov.au/def/gn-part-types>" ;
.
